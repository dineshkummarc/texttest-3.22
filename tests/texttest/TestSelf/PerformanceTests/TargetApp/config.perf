import_config_file:$STANDARD

# Path to the executable
executable:${TEXTTEST_HOME}/writeperfnumbers.py

default_interface:console

# ------ Performance settings -------

[run_dependent_text]
output:cpu time
errors:cpu time

[performance_logfile]
performance:output
performance:erro*

[performance_logfile_extractor]
performance:cpu time:

# Which machine to check performance on
[performance_test_machine]
performance:any

# Use collection for self tests, so we can compare reports
[batch_use_collection]
default:true
